<sparrow-reach-request
  xmlns="http://www.usgs.gov/sparrow/id-point-request/v0_2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<!-- The user does not have a prediction context yet, so we use the model-id here -->
	<model-id>22</model-id>
	<!--  if prediction context were available, it would look like this
	<context-id>720751343</context-id> -->

	<!-- Where the user clicked -->
	<!-- <point lat="39.5" long="-76.68"/> -->

	<!-- or try a reach rather than a point -->
	<!-- <reach id="3074"/> -->

	<!-- or try a broader query -->
	<match-query>  <!-- specify 1+ of these sub elements -->
		<reach-id>4332</reach-id>
		<reach-name>wolf</reach-name>
		<meanQHi>1234</meanQHi>
		<meanQLo>800</meanQLo>
		<catch-area-hi>2345</catch-area-hi>
		<catch-area-lo>1</catch-area-lo>
		<huc>hucname</huc>
	</match-query>


	<!-- Ask for just the info on the adjustments tab. -->
	<content>
		<!-- <adjustments/> should now contain adequate info for "find reach" query -->
		<!-- <attributes/> uncomment this to get attributes -->
		<!-- <predicted/>  uncomment this to get predicted -->
	</content>


	<!-- Get the response back as JSON or XML -->
	<response-format>
		<mime-type>XML</mime-type>
	</response-format>
</sparrow-reach-request>
