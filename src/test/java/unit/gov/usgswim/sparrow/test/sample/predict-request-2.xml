<?xml version="1.0" encoding="ISO-8859-1" ?>
<sparrow-prediction-request
  xmlns="http://www.usgs.gov/sparrow/prediction-request/v0_1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<predict model-id="22">
		<change-from-nominal type="perc_change">
			<source-adjustments>
				<gross-src src="1" coef=".5"/>
				<gross-src src="4" coef="2"/>
			</source-adjustments>
		</change-from-nominal>
	</predict>
	
	<response-options>
		<result-filter>
			<near-point result-count="5">
				<point lat="40" long="-100"/>
			</near-point>
		</result-filter>
		<result-content>
			<dataSeries>incremental</dataSeries>
		</result-content>
	</response-options>

</sparrow-prediction-request>