					<criteria attrib="huc8">12050002</criteria>
					<!-- CONTAINS REACHES:
						39529
						39526
						39528
						39527
					-->
				</logicalSet>
				<logicalSet>
					<criteria attrib="huc8">12080001</criteria>
					<!-- CONTAINS REACHES:
						39966
						39968
						39967
						39965
					-->
				</logicalSet>
